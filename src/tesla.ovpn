client
dev tun8
tun-mtu 1500
proto udp
mssfix 1260
sndbuf 0
rcvbuf 0
remote YOURIPADDRESSHERE 1194
resolv-retry 60
log-append /var/log/openvpn.log
nobind
persist-key
persist-tun
persist-remote-ip
script-security 2
ping 10
ping-exit 60
auth-nocache
remote-cert-tls server
auth SHA512
cipher AES-256-CBC
setenv opt block-outside-dns
key-direction 1
###CERTSHERE###
